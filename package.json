{
    "name": "bank-payment-validator",
    "version": "2022.0.0",
    "description": "code bank payment validator",
    "private": true,
    "scripts": {
        "test": "jest --no-cache",
        "coverage": "npx live-server ./coverage/lcov-report",
        "start": "fastify start -l info dist/src/app.js",
        "start:local": "npm run build:ts && fastify start -l info dist/app.js",
        "build:ts": "tsc",
        "lint": "eslint --ext .ts,.tsx . --max-warnings 0 --fix",
        "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P dist/src/app.js\""
    },
    "keywords": [],
    "author": "bearkfear",
    "license": "ISC",
    "dependencies": {
        "@fastify/autoload": "^4.0.1",
        "@fastify/cors": "^7.0.0",
        "@fastify/sensible": "^4.1.0",
        "@fastify/swagger": "^6.0.1",
        "axios": "0.27.2",
        "date-fns": "^2.28.0",
        "fastify": "3.29.0",
        "@fastify/autoload": "^4.0.0",
        "fastify-cli": "3.1.0",
        "fastify-cors": "6.1.0",
        "fastify-plugin": "3.0.1",
        "fastify-sensible": "3.2.0",
        "fastify-swagger": "5.2.0",
        "json-schema-to-ts": "2.4.0",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
